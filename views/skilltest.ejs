<% data.forEach(function(data,i){%>
<div style="flex-wrap: wrap;justify-content: center;max-width: 600px;margin: auto; " class="">
   
    
        <input type="hidden" name="skillid"  class="form-control" id="proid1" aria-describedby="emailHelp" placeholder="Enter Skill">
        <input type="hidden" name="correct<%=i%>"  class="form-control" id="correct<%=i%>" aria-describedby="emailHelp" placeholder="Enter Skill" value="<%=data.correct%>">
        
        <div class="form-group">
          <label style="font-size: 24px;" for="exampleInputEmail1">Question</label>
          <h2 class="fs1"><%=data.question%></h2>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="radio" name="flexRadioDefault<%=i%>" value="0" id="flexRadioDefault<%=i%>">
            <label class="form-check-label" for="flexRadioDefault1">
                <%=data.option1%>
            </label>
          </div>
          <div class="form-check">
            <input class="form-check-input" type="radio" name="flexRadioDefault<%=i%>" value="1" id="flexRadioDefault<%=i%>" >
            <label class="form-check-label" for="flexRadioDefault2">
                <%=data.option2%>
            </label>
          </div>
          <div class="form-check">
            <input class="form-check-input" type="radio" name="flexRadioDefault<%=i%>" value="2" id="flexRadioDefault<%=i%>" >
            <label class="form-check-label" for="flexRadioDefault2">
                <%=data.option3%>
            </label>
          </div>
          <div class="form-check">
